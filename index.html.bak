      function renderNoticeList(page) {
        const tbody = document.getElementById('notice-list-body');
        if (!tbody) return;
        tbody.innerHTML = '';

        const start = (page - 1) * itemsPerPage;
        const end = start + itemsPerPage;
        const items = noticesData.slice(start, end);

        items.forEach(n => {
          let typeLabel = '';
          if (n.type === '공지') {
            typeLabel = '<span style="background:#f97316; color:#fff; font-size:0.75rem; font-weight:700; padding:0.125rem 0.375rem; border-radius:0.25rem;">공지</span>';
          } else {
            typeLabel = `<span style="font-weight:500; color:#94a3b8;">${n.id}</span>`;
          }

          let statusLabel = '';
          if (n.status) {
            let bg = '#f1f5f9', color = '#64748b';
            if (n.status === '중요') { bg = '#fee2e2'; color = '#dc2626'; }
            else if (n.status === '이벤트') { bg = '#fce7f3'; color = '#db2777'; }
            else if (n.status === '예정') { bg = '#f1f5f9'; color = '#64748b'; }
            statusLabel = `<span style="background:${bg}; color:${color}; font-size:0.625rem; font-weight:700; padding:0.125rem 0.375rem; border-radius:0.25rem; margin-left:0.5rem;">${n.status}</span>`;
          }

          let rowBg = n.type === '공지' ? '#fff7ed' : (n.id % 2 === 0 ? '#f8fafc' : '#ffffff');
          if (n.type !== '공지') rowBg = '#ffffff'; // 깔끔하게 흰색/회색 교차보다는 그냥 구분선으로

          const tr = document.createElement('tr');
          tr.style.borderBottom = '1px solid #f8fafc';
          if (n.type === '공지') tr.style.backgroundColor = '#fff7ed';
          tr.style.cursor = 'pointer';
          tr.onclick = () => openNoticeModal(n.id);

          tr.innerHTML = `
            <td style="padding:1.25rem 1.5rem; text-align:center;">${typeLabel}</td>
            <td style="padding:1.25rem 1.5rem;">
              <div style="display:flex; align-items:center;">
                <span style="font-weight:${n.type === '공지' ? '700' : '500'}; color:#1e293b;">${n.title}</span>
                ${statusLabel}
              </div>
            </td>
            <td style="padding:1.25rem 1.5rem; text-align:center; color:#64748b;">${n.author}</td>
            <td style="padding:1.25rem 1.5rem; text-align:center; color:#94a3b8;">${n.date}</td>
            <td style="padding:1.25rem 1.5rem; text-align:center; color:#cbd5e1;">${n.views}</td>
          `;
          tbody.appendChild(tr);
        });

        // Update Buttons
        const btn1 = document.getElementById('page-btn-1');
        const btn2 = document.getElementById('page-btn-2');
        if (btn1) {
          btn1.style.background = page === 1 ? '#2563eb' : '#fff';
          btn1.style.color = page === 1 ? '#fff' : '#94a3b8';
          btn1.style.border = page === 1 ? 'none' : '1px solid #e2e8f0';
        }
        if (btn2) {
          btn2.style.background = page === 2 ? '#2563eb' : '#fff';
          btn2.style.color = page === 2 ? '#fff' : '#94a3b8';
          btn2.style.border = page === 2 ? 'none' : '1px solid #e2e8f0';
        }
        currentPage = page;
      }

      function changePage(page) {
        if (page < 1 || page > 2) return;
        renderNoticeList(page);
      }

      function openNoticeModal(id) {
        const notice = noticesData.find(n => n.id === id);
        if (!notice) return;

        document.getElementById('modal-type').textContent = notice.type;
        const statusEl = document.getElementById('modal-status');
        if (notice.status) {
          statusEl.textContent = notice.status;
          statusEl.style.display = 'inline-block';
          if (notice.status === '중요') { statusEl.style.background = '#fee2e2'; statusEl.style.color = '#dc2626'; }
          else if (notice.status === '예정') { statusEl.style.background = '#f1f5f9'; statusEl.style.color = '#64748b'; }
          else if (notice.status === '이벤트') { statusEl.style.background = '#fce7f3'; statusEl.style.color = '#db2777'; }
          else { statusEl.style.background = '#f1f5f9'; statusEl.style.color = '#64748b'; }
        } else {
          statusEl.style.display = 'none';
        }

        document.getElementById('modal-title').textContent = notice.title;
        document.getElementById('modal-author').textContent = notice.author;
        document.getElementById('modal-date').textContent = notice.date;
        document.getElementById('modal-views').textContent = notice.views;
        document.getElementById('modal-body').innerText = notice.content || '내용이 없습니다.';

        const modal = document.getElementById('notice-modal');
        modal.style.display = 'block';
        document.body.style.overflow = 'hidden';
      }

      function closeNoticeModal() {
        const modal = document.getElementById('notice-modal');
        modal.style.display = 'none';
        document.body.style.overflow = '';
      }

      window.onclick = function (event) {
        const modal = document.getElementById('notice-modal');
        if (event.target == modal) {
          closeNoticeModal();
        }
      }

      // FAQ Logic
      const faqData = [
        { q: "Q. 프로그램 신청 및 분석 시작은 어떻게 하나요?", a: "홈페이지 내 [온라인 신청] 메뉴를 통해 희망하시는 로드맵 프로그램을 선택하여 예약하실 수 있습니다. 신청이 완료되면 담당 설계사가 확인 후 개별 안내를 드립니다." },
        { q: "Q. 각 로드맵 프로그램의 비용 체계가 궁금합니다.", a: "그로우그래프는 회기 수와 목적에 따라 정찰제로 운영됩니다.\n• G-Check(1회): 8만 원\n• G-Boost(5회): 40만 원\n• G-Restore(8회): 65만 원\n• G-Mastery(3개월): 100만 원\n• G-Share(워크숍): 1회 4만 원" },
        { q: "Q. 세션당 소요 시간과 전체 기간은 어떻게 되나요?", a: "모든 1:1 세션은 1회당 약 50~60분 동안 집중적으로 진행됩니다. 프로그램의 성격에 따라 1회 단기 진단부터 3개월 이상의 장기 프로젝트까지 구성이 상이합니다." },
        { q: "Q. 오프라인 코칭 시 주차 공간이 확보되어 있나요?", a: "각 거점별 주차 안내는 다음과 같습니다.\n• 구로점: 건물 내 전용 주차장 이용이 가능합니다.\n• 여의도·홍대점: 인근 공영 주차장 이용을 권장해 드립니다." },
        { q: "Q. 그로우그래프의 운영 시간은 어떻게 되나요?", a: "평일 저녁과 주말 세션을 모두 운영하고 있습니다.\n• 평일: 09:00 ~ 20:00\n• 주말: 10:00 ~ 18:00\n*공휴일은 휴무입니다." },
        { q: "Q. 개인의 성장 데이터와 상담 내용은 보안이 유지되나요?", a: "네, 철저히 보장됩니다. 그로우그래프는 엄격한 윤리 규정을 준수하며, 수집된 모든 데이터는 오직 본인의 로드맵 설계 목적으로만 활용되고 안전하게 보호됩니다." },
        { q: "Q. 프로그램 참여 전 미리 준비하거나 제출해야 할 자료가 있나요?", a: "특별히 준비하실 것은 없습니다. 다만, 보다 정밀한 진단을 위해 홈페이지의 [자가진단 도구]를 미리 활용해 보고 오시면 첫 세션에서 더욱 깊이 있는 데이터를 확인하실 수 있습니다." }
      ];

      function renderFAQ() {
        const container = document.getElementById('faq-list-container');
        if (!container) return;
        container.innerHTML = '';

        faqData.forEach((item, index) => {
          const div = document.createElement('div');
          div.style.borderBottom = '1px solid #e2e8f0';
          div.innerHTML = `
            <button onclick="toggleFAQ(${index})" style="width:100%; padding:1.5rem 0; display:flex; justify-content:space-between; align-items:center; text-align:left; background:none; border:none; cursor:pointer;">
              <span id="faq-q-${index}" style="font-size:1.125rem; font-weight:600; color:#1e293b; transition:color 0.2s;">${item.q}</span>
              <svg id="faq-icon-${index}" style="width:1.5rem; height:1.5rem; color:#94a3b8; transition:transform 0.3s, color 0.3s;" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"></path>
              </svg>
            </button>
            <div id="faq-a-${index}" style="max-height:0; overflow:hidden; transition:max-height 0.3s ease-in-out;">
              <p style="padding-bottom:1.5rem; color:#475569; line-height:1.7; white-space:pre-line;">${item.a}</p>
            </div>
          `;
          container.appendChild(div);
        });
      }

      function toggleFAQ(index) {
        const answerDiv = document.getElementById(`faq-a-${index}`);
        const icon = document.getElementById(`faq-icon-${index}`);
        const question = document.getElementById(`faq-q-${index}`);

        if (answerDiv.style.maxHeight === '0px' || !answerDiv.style.maxHeight) {
          answerDiv.style.maxHeight = answerDiv.scrollHeight + 'px';
          icon.style.transform = 'rotate(180deg)';
          icon.style.color = '#2563eb';
          question.style.color = '#2563eb';
        } else {
          answerDiv.style.maxHeight = '0px';
          icon.style.transform = 'rotate(0deg)';
          icon.style.color = '#94a3b8';
          question.style.color = '#1e293b';
        }
      }

      // Initial Render
      setTimeout(() => {
        renderNoticeList(1);
        renderFAQ();
      }, 100);

      function manageContent() {
        const path = window.location.pathname;
        const isStoryPage = path.includes('/intro/story');
        const isPhiloPage = path.includes('/intro/philosophy');
        const isCorpPage = path.includes('/b2b/corporate');
        const isCampusPage = path.includes('/b2b/campus');
        const isSeminarPage = path.includes('/b2b/seminar');
        const isNoticePage = path.includes('/community/notice');
        const isFAQPage = path.includes('/community/faq');
        const isSpecialPage = isStoryPage || isPhiloPage || isCorpPage || isCampusPage || isSeminarPage || isNoticePage || isFAQPage;

        const newStorySection = document.getElementById('new-story-section');
        const newPhiloSection = document.getElementById('new-philosophy-section');
        const newCorpSection = document.getElementById('new-corporate-section');
        const newCampusSection = document.getElementById('new-campus-section');
        const newSeminarSection = document.getElementById('new-seminar-section');
        const newNoticeSection = document.getElementById('new-notice-section');
        const newFAQSection = document.getElementById('new-faq-section');

        // 1. Manage Global Visibility (Main Content vs Special Content)
        // Signatures for both Main Content (CEO Message) and Footer (Growth Visualized)
        const signatures = ['CEO MESSAGE', '진정한 성장은', '김세희', 'Growth Visualized', 'Instagram', 'All rights reserved'];

        // We scan primarily for text containers
        const candidates = document.querySelectorAll('h1, h2, h3, h4, div, section, footer, p, span');

        for (const el of candidates) {
          if (el.textContent && signatures.some(sig => el.textContent.includes(sig))) {
            // Safety: Exclude our new sections and core UI elements
            if (newStorySection && newStorySection.contains(el)) continue;
            if (newPhiloSection && newPhiloSection.contains(el)) continue;
            if (newCorpSection && newCorpSection.contains(el)) continue;
            if (newCampusSection && newCampusSection.contains(el)) continue;
            if (newSeminarSection && newSeminarSection.contains(el)) continue;
            if (newNoticeSection && newNoticeSection.contains(el)) continue;
            if (newFAQSection && newFAQSection.contains(el)) continue;
            if (el.classList.contains('navbar') || el.closest('.navbar')) continue;
            if (el.id === 'root') continue;
            if (el.tagName === 'BODY' || el.tagName === 'HTML') continue;

            // Identify the Container to Hide/Show
            let target = null;

            // A. Footer Detection
            if (el.textContent.includes('Growth Visualized') || el.textContent.includes('Instagram')) {
              let parent = el;
              for (let i = 0; i < 8; i++) {
                if (!parent || parent === document.body || parent.id === 'root') break;
                // Heuristic for Footer
                if (parent.tagName === 'FOOTER' || parent.classList.contains('footer') || window.getComputedStyle(parent).backgroundColor === 'rgb(15, 23, 42)') {
                  target = parent;
                  break;
                }
                parent = parent.parentElement;
              }
            }
            // B. Standard Section Detection
            else {
              let parent = el;
              for (let i = 0; i < 6; i++) {
                if (!parent || parent === document.body) break;
                if (parent.tagName === 'SECTION' || parent.classList.contains('section') || parent.className.includes('py-')) {
                  target = parent;
                  break;
                }
                parent = parent.parentElement;
              }
            }

            // Toggle Visibility
            if (target) {
              if (isSpecialPage) {
                target.style.display = 'none';
              } else {
                target.style.display = ''; // Restore visibility on Main Page
              }
            }
          }
        }

        // 2. Manage New Sections Visibility
        if (newStorySection) {
          newStorySection.style.display = isStoryPage ? 'block' : 'none';
        }
        if (newPhiloSection) {
          newPhiloSection.style.display = isPhiloPage ? 'block' : 'none';
        }
        if (newCorpSection) {
          newCorpSection.style.display = isCorpPage ? 'block' : 'none';
        }
        if (newCampusSection) {
          newCampusSection.style.display = isCampusPage ? 'block' : 'none';
        }
        if (newSeminarSection) {
          newSeminarSection.style.display = isSeminarPage ? 'block' : 'none';
        }
        if (newNoticeSection) {
          newNoticeSection.style.display = isNoticePage ? 'block' : 'none';
        }
        if (newFAQSection) {
          newFAQSection.style.display = isFAQPage ? 'block' : 'none';
        }

        // Background check
        if (isSpecialPage) {
          document.body.style.backgroundColor = '#ffffff';
        } else {
          document.body.style.backgroundColor = ''; // Restore
        }
      }

      // Menu Link Fix Logic
      function fixMenuLinks() {
        // Target dropdown items specifically or nav links
        const links = document.querySelectorAll('a, li');

        links.forEach(link => {
          const text = link.textContent.trim();

          if (text === '그로우그래프 스토리' || text.includes('스토리')) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/intro/story';
            };
          }

          if (text === '기업이념' || (text.includes('기업') && text.includes('이념'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/intro/philosophy';
            };
          }

          if (text === '기업 코칭' || text === '기업용' || text === '기업' || (text.includes('기업') && text.includes('코칭'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/b2b/corporate';
            };
          }

          if (text === '교육기관' || text === '캠퍼스' || (text.includes('교육') && text.includes('기관'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/b2b/campus';
            };
          }

          if (text === '강연및세미나' || text === '세미나' || text === '강연' || (text.includes('강연') && text.includes('세미나'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/b2b/seminar';
            };
          }

          if (text === '공지사항' || text === '공지' || (text.includes('공지') && text.includes('사항'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/community/notice';
            };
          }

          if (text === '자주하는질문' || text === 'FAQ' || (text.includes('자주') && text.includes('질문'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/community/faq';
            };
          }
        });
      }

      // Scroll Animation Logic
      function handleScrollAnimations() {
        const observerOptions = {
          root: null,
          rootMargin: '0px',
          threshold: 0.1
        };

        const observer = new IntersectionObserver((entries, observer) => {
          entries.forEach(entry => {
            if (entry.isIntersecting) {
              entry.target.classList.add('is-visible');
              observer.unobserve(entry.target); // Animate once
            }
          });
        }, observerOptions);

        const elements = document.querySelectorAll('.reveal-on-scroll');
        elements.forEach(el => observer.observe(el));
      }

      window.addEventListener('load', updateLogo);
      window.addEventListener('DOMContentLoaded', updateLogo);

      const observer = new MutationObserver((mutations) => {
        updateLogo();
        manageContent();
        fixMenuLinks();
        handleScrollAnimations();
      });
      observer.observe(document.body, { childList: true, subtree: true });

      // Run repeatedly
      setInterval(() => {
        manageContent();
        fixMenuLinks();
        handleScrollAnimations();
      }, 500);

      // Handle navigation
      const originalPushState = history.pushState;
      history.pushState = function () {
        originalPushState.apply(this, arguments);
        setTimeout(manageContent, 50);
        setTimeout(fixMenuLinks, 50);
      };
      window.addEventListener('popstate', manageContent);
      window.addEventListener('load', manageContent);

      manageContent();
      fixMenuLinks();

    })();
  </script>
</body>

</html>