          <div style="height: 0.5rem; background: #f1f5f9; border-radius: 9999px; overflow: hidden;">
            <div style="width: ${progress}%; height: 100%; background: #2563eb; transition: width 0.3s;"></div>
          </div>
        `;
      }

      function nextDiagQuestion(index, val) {
        currentDiagScores[index] = val;
        setTimeout(() => {
          renderDiagQuestion(index + 1);
        }, 300); // Slight delay for UX
      }

      function showDiagResult() {
        const totalScore = currentDiagScores.reduce((a, b) => a + b, 0);
        const data = diagnosticData[currentDiagType];

        // Determine result range
        const result = data.ranges.find(r => totalScore <= r.max) || data.ranges[data.ranges.length - 1];

        const container = document.getElementById('diag-content');
        container.innerHTML = `
          <div style="text-align: center;">
            <h2 style="font-size: 1.5rem; font-weight: 700; color: #1e293b; margin-bottom: 2rem;">진단 결과</h2>
            
