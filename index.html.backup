      function startDiagnostic(type) {
        currentDiagType = type;
        currentDiagScores = new Array(diagnosticData[type].questions.length).fill(null);
        renderDiagQuestion(0);

        const modal = document.getElementById('diag-modal');
        modal.style.display = 'block';
        document.body.style.overflow = 'hidden';
      }

      function renderDiagQuestion(index) {
        const data = diagnosticData[currentDiagType];
        const container = document.getElementById('diag-content');

        // Check if finished
        if (index >= data.questions.length) {
          showDiagResult();
          return;
        }

        const progress = ((index + 1) / data.questions.length) * 100;

        container.innerHTML = `
          <div style="margin-bottom: 2rem;">
            <div style="font-size: 0.875rem; color: #64748b; font-weight: 600; margin-bottom: 0.5rem;">${data.title}</div>
            <div style="font-size: 1.5rem; font-weight: 700; color: #1e293b; line-height: 1.4; word-break: keep-all;">
              Q${index + 1}. ${data.questions[index]}
            </div>
          </div>
          
          <div style="display: flex; flex-direction: column; gap: 0.75rem; margin-bottom: 3rem;">
            ${[0, 1, 2, 3].map((val, i) => `
              <div style="position: relative;">
                <input type="radio" name="q${index}" id="opt${i}" value="${val}" style="position: absolute; opacity: 0; width: 0; height: 0;" onchange="nextDiagQuestion(${index}, ${val})">
