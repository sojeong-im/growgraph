                    { tag: '모집', text: "'데이터로 증명하는 리더의 성장 궤적' 공개 세미나 신청 오픈" },
                    { tag: '안내', text: "'구조적 로드맵이 만드는 조직의 변화' 하반기 컨설팅 사전 안내" }
                  ];

                  list.innerHTML = newNotices.map(n => `
                     <li class="notice-item" style="cursor: pointer; margin-bottom: 0.75rem; display: flex; align-items: flex-start;">
                       <span class="notice-tag" style="background-color: ${n.tag === '공지' ? '#f97316' : '#64748b'}; color: white; padding: 0.15rem 0.5rem; border-radius: 0.25rem; font-size: 0.75rem; font-weight: 700; margin-right: 0.5rem; white-space: nowrap; margin-top: 0.15rem;">${n.tag}</span>
                       <span class="notice-text" style="font-size: 0.95rem; color: #334155; line-height: 1.5;">${n.text}</span>
                     </li>
                   `).join('');
                  container.dataset.updatedNotices = 'true';
                }
              }
            }
          }

          // 4-2. Enhance Hero Badge ("나의 성장 경로 발견")
          // Optimized Hero Badge Logic (XPath)
          const xpath = "//*[text()='나의 성장 경로 발견']";
          const badgeResult = document.evaluate(xpath, document.body, null, XPathResult.ORDERED_NODE_SNAPSHOT_TYPE, null);
          for (let i = 0; i < badgeResult.snapshotLength; i++) {
            const el = badgeResult.snapshotItem(i);
            if (el.dataset.enhancedBadge) continue;
            if (el.closest('nav') || el.closest('header')) continue;

            el.innerHTML = `
                     <div style="display: inline-flex; align-items: center; gap: 0.6rem; padding: 0.5rem 1.25rem; background: rgba(255, 255, 255, 0.9); backdrop-filter: blur(8px); border: 1px solid rgba(59, 130, 246, 0.2); border-radius: 9999px; box-shadow: 0 4px 20px -2px rgba(59, 130, 246, 0.15); transition: transform 0.3s ease;">
                       <span style="position: relative; display: flex; height: 8px; width: 8px;">
                          <span style="animation: ping 2s cubic-bezier(0, 0, 0.2, 1) infinite; position: absolute; display: inline-flex; height: 100%; width: 100%; border-radius: 50%; background-color: #60a5fa; opacity: 0.75;"></span>
                          <span style="position: relative; display: inline-flex; border-radius: 50%; height: 8px; width: 8px; background-color: #2563eb;"></span>
                       </span>
                       <span style="font-size: 0.9rem; font-weight: 700; color: #1e293b; background: linear-gradient(to right, #1e293b, #475569); -webkit-background-clip: text; -webkit-text-fill-color: transparent;">나의 성장 경로 발견</span>
                     </div>
                     <style>@keyframes ping { 75%, 100% { transform: scale(2); opacity: 0; } }</style>
             `;
            el.style.backgroundColor = 'transparent'; el.style.padding = '0'; el.style.border = 'none'; el.style.display = 'inline-block';
            el.dataset.enhancedBadge = 'true';
            console.log('Badge enhanced');
          }
          const possibleBadges = []; // Disable original loop to prevent performance issues
          for (const el of possibleBadges) {
            // Check text content carefully to avoid replacing large containers
            if (el.innerText && el.innerText.trim() === '나의 성장 경로 발견' && !el.closest('nav') && !el.closest('header')) {
              if (el.dataset.enhancedBadge) continue;
              // Ensure it's a leaf element or very close to it (text node wrapper)
              if (el.children.length <= 1) {
                el.innerHTML = `
                     <div style="display: inline-flex; align-items: center; gap: 0.6rem; padding: 0.5rem 1.25rem; background: rgba(255, 255, 255, 0.9); backdrop-filter: blur(8px); border: 1px solid rgba(59, 130, 246, 0.2); border-radius: 9999px; box-shadow: 0 4px 20px -2px rgba(59, 130, 246, 0.15); transition: transform 0.3s ease;">
                       <span style="position: relative; display: flex; height: 8px; width: 8px;">
                          <span style="animation: ping 2s cubic-bezier(0, 0, 0.2, 1) infinite; position: absolute; display: inline-flex; height: 100%; width: 100%; border-radius: 50%; background-color: #60a5fa; opacity: 0.75;"></span>
                          <span style="position: relative; display: inline-flex; border-radius: 50%; height: 8px; width: 8px; background-color: #2563eb;"></span>
                       </span>
                       <span style="font-size: 0.9rem; font-weight: 700; color: #1e293b; background: linear-gradient(to right, #1e293b, #475569); -webkit-background-clip: text; -webkit-text-fill-color: transparent;">나의 성장 경로 발견</span>
                     </div>
                     <style>@keyframes ping { 75%, 100% { transform: scale(2); opacity: 0; } }</style>
                   `;
                // Reset parent styles
                el.style.backgroundColor = 'transparent';
                el.style.padding = '0';
                el.style.border = 'none';
                el.style.display = 'inline-block';

                el.dataset.enhancedBadge = 'true';
              }
            }
          }

          // 5. Update Hero Button Link ("더 알아보기")
          const links = document.querySelectorAll('a');
          for (const link of links) {
            if (link.textContent.includes('더 알아보기') || link.textContent.includes('자세히 보기')) {
              // Only update if it's a dummy link
              if (link.getAttribute('href') === '#' || link.getAttribute('href') === '' || !link.getAttribute('href')) {
                link.href = '/program/info';
              }
            }
          }

          // 6. Update Contact/Partnership Links // NEW BLOCK
          const contactKeywords = ['문의하기', '제휴', '강연', '파트너십']; // Added '파트너십'
          for (const link of links) {
            const text = link.textContent;
            if (contactKeywords.some(kw => text.includes(kw))) {
              if (!link.getAttribute('href') || link.getAttribute('href') === '#' || link.getAttribute('href').includes('mailto')) {
                link.href = '/apply/partnership';
              }
            }
          }
        }
      }

      // Menu Link Fix Logic
      function fixMenuLinks() {
        // Target dropdown items specifically or nav links
        const links = document.querySelectorAll('nav a, nav li, nav button, header a, header li, header button, .navbar a, .navbar button, .navbar li, li, a, button');

        links.forEach(link => {
          const text = link.textContent.trim();

          if (text === '그로우그래프 스토리' || text.includes('스토리')) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/intro/story';
            };
          }

          if (text === '기업이념' || (text.includes('기업') && text.includes('이념'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/intro/philosophy';
            };
          }

          if (text === '기업 코칭' || text === '기업용' || text === '기업' || (text.includes('기업') && text.includes('코칭'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/b2b/corporate';
            };
          }

          if (text === '교육기관' || text === '캠퍼스' || (text.includes('교육') && text.includes('기관'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/b2b/campus';
            };
          }

          if (text === '강연및세미나' || text === '세미나' || text === '강연' || (text.includes('강연') && text.includes('세미나'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/b2b/seminar';
            };
          }

          if (text === '공지사항' || text === '공지' || (text.includes('공지') && text.includes('사항'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/community/notice';
            };
          }

          if (text === '자주하는질문' || text === 'FAQ' || (text.includes('자주') && text.includes('질문'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/community/faq';
            };
          }

          if (text === '로그인' || text === 'Login' || link.classList.contains('login-btn')) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/auth/login';
            };
          }

          if (text === '자가진단' || text.includes('진단')) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/community/diagnostic';
            };
          }

          if (text === '그로우그래프 공간' || text === '공간' || (text.includes('공간') && text.includes('소개'))) {
            link.style.cursor = 'pointer';
            link.onclick = (e) => {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = '/intro/space';
            };
          }
        });
      }

      // Scroll Animation Logic
      function handleScrollAnimations() {
        const observerOptions = {
          root: null,
          rootMargin: '0px',
          threshold: 0.1
        };

        const observer = new IntersectionObserver((entries, observer) => {
          entries.forEach(entry => {
            if (entry.isIntersecting) {
              entry.target.classList.add('is-visible');
              observer.unobserve(entry.target); // Animate once
            }
          });
        }, observerOptions);

        const elements = document.querySelectorAll('.reveal-on-scroll');
        elements.forEach(el => observer.observe(el));
      }

      window.addEventListener('load', updateLogo);
      window.addEventListener('DOMContentLoaded', updateLogo);

      const observer = new MutationObserver((mutations) => {
        updateLogo();
        manageContent();
        fixMenuLinks();
        handleScrollAnimations();
      });
      observer.observe(document.body, { childList: true, subtree: true });

      // Run repeatedly
      setInterval(() => {
        manageContent();
        fixMenuLinks();
        handleScrollAnimations();
      }, 500);

      // Handle navigation
      const originalPushState = history.pushState;
      history.pushState = function () {
        originalPushState.apply(this, arguments);
        setTimeout(manageContent, 50);
        setTimeout(fixMenuLinks, 50);
      };
      window.addEventListener('popstate', manageContent);
      window.addEventListener('load', manageContent);

      manageContent();
      fixMenuLinks();

    })();
  </script>
</body>

