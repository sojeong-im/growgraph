  <script>
    (function () {
      function updateLogo() {
        const logoContainer = document.querySelector('.navbar-logo');
        if (logoContainer && !logoContainer.querySelector('.custom-logo')) {
          logoContainer.textContent = ''; 
          const img = document.createElement('img');
          img.src = '/logo_new.png';
          img.alt = 'GrowGraph';
          img.className = 'custom-logo';
          logoContainer.appendChild(img);
        }
      }

      function manageContent() {
        const path = window.location.pathname;
        const isStoryPage = path.includes('/intro/story');
        const newStorySection = document.getElementById('new-story-section');
        
        // 1. Hide Old Content (CEO MESSAGE, etc)
        const oldContentSignatures = ['CEO MESSAGE', '진정한 성장은', '김세희'];
        const candidates = document.querySelectorAll('h1, h2, h3, h4, div, section');
        
        for (const el of candidates) {
          // Check if this element contains our target text
          if (el.textContent && oldContentSignatures.some(sig => el.textContent.includes(sig))) {
             // Avoid hiding our NEW section or Navbar
             if (newStorySection && newStorySection.contains(el)) continue;
             if (el.classList.contains('navbar') || el.closest('.navbar')) continue;
             
             // Find the section wrapper to hide
             let parent = el;
             // Climb up to find the visible Section/Container
             for(let i=0; i<6; i++) {
                if(!parent || parent === document.body) break;
                // Heuristic: Section tag or specific classes often used in main page sections
                if(parent.tagName === 'SECTION' || parent.classList.contains('section') || parent.className.includes('py-')) {
                   parent.style.display = 'none'; 
                   break;
                }
                parent = parent.parentElement;
             }
          }
        }

        // 2. Manage New Story Section Visibility
        if (newStorySection) {
          if (isStoryPage) {
            newStorySection.style.display = 'block';
            document.body.style.backgroundColor = '#ffffff';
          } else {
            newStorySection.style.display = 'none';
          }
        }
      }

      window.addEventListener('load', updateLogo);
      window.addEventListener('DOMContentLoaded', updateLogo);
      
      const observer = new MutationObserver((mutations) => {
        updateLogo();
        manageContent();
      });
      observer.observe(document.body, { childList: true, subtree: true });

      // Run repeatedly
      setInterval(manageContent, 200);

      // Handle navigation
      const originalPushState = history.pushState;
      history.pushState = function () {
        originalPushState.apply(this, arguments);
        setTimeout(manageContent, 50);
      };
      window.addEventListener('popstate', manageContent);
      window.addEventListener('load', manageContent);

      manageContent();

    })();
  </script>
